<UserControl
  d:DesignHeight="450"
  d:DesignWidth="800"
  mc:Ignorable="d"
  x:Class="SpotifyDataExplorer.Views.Pages.HistoryPageView"
  x:DataType="pages:HistoryPageViewModel"
  xmlns="https://github.com/avaloniaui"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:pages="clr-namespace:SpotifyDataExplorer.ViewModels.Pages"
  xmlns:panels="clr-namespace:SpotifyDataExplorer.Views.Panels"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <Design.DataContext>
    <pages:HistoryPageViewModel />
  </Design.DataContext>

  <Grid RowDefinitions="Auto, *">
    <Button
      Command="{Binding LastScreenCmd}"
      Content="Page Back"
      Grid.Row="0"
      Height="50"
      HorizontalAlignment="Left"
      VerticalAlignment="Center"
      VerticalContentAlignment="Center" />

    <panels:HistoryView DataContext="{Binding HistoryViewModel}" Grid.Row="1" />
  </Grid>

</UserControl>